@model IEnumerable<WebMVC_eSport.Models.EQUIPO>

@{
    ViewBag.Title = "Index";
}

<h2>Mantenimiento de Equipo</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.IdEquipo)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.NomEquipo)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.PaisEquipo)
        </th>

    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.NomEquipo)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.PaisEquipo)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.IdEquipo }) |
            @Html.ActionLink("Details", "Details", new { id=item.IdEquipo }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.IdEquipo })
        </td>
    </tr>
}

</table>
